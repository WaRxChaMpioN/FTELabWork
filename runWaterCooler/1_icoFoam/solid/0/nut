FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    out
    {
        type            zeroGradient;
    }
    interfaceOut
    {
        type                mapped;
   //     field                 U;
        value               uniform 0;
        interpolationScheme cell;
        setAverage          false;
       average             0;
    }
    fixedWalls
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


