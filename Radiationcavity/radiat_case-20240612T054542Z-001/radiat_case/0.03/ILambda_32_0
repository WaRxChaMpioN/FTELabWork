{
    fixedWall_Bottom
    {
        type            greyDiffusiveRadiation;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 0;
        source          uniform 0;
        value           uniform 0;
    }
    fixedWall_Right
    {
        type            greyDiffusiveRadiation;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 1;
        source          uniform 0;
        value           uniform 0;
    }
    fixedWall_Left
    {
        type            greyDiffusiveRadiation;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 0;
        source          uniform 0;
        value           uniform 0;
    }
    fixedWall_Top
    {
        type            greyDiffusiveRadiation;
        refValue        uniform 0;
        refGradient     uniform 0;
        valueFraction   uniform 1;
        source          uniform 0;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


