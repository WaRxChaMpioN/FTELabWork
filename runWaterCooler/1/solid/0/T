FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    out
    {
        type            fixedValue;
        value           uniform 300;
    }
    /*
    InterFaceOut
    {
        type            compressible::turbulentTemperatureCoupledMixed;
        value           $internalField;
        Tnbr            T;
        kappaMethod     fluidThermo;
    } */
    interfaceOut
    {
        type            mapped;
        value           uniform 300;
        interpolationScheme cell;
        setAverage          false;
        average             300;
    }
    fixedWalls
    {
        type            fixedGradient;
        gradient           uniform 20000;
    }
    frontAndBack
    {
        type            empty;
    }
}




