FoamFile
{
    version     2.0;
    format      binary;
    arch        "LSB;label=32;scalar=64";
    class       polyBoundaryMesh;
    location    "constant/zonetwo/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

6
(
    zonetwo_to_zoneone
    {
        type            mappedPatch;
        inGroups        1(mappedPatch);
        nFaces          10;
        startFace       7590;
        sampleMode      nearestPatchFace;
        sampleRegion    zoneone;
        samplePatch     zoneone_to_zonetwo;
        sampleDatabase  true;
    }
    outlet
    {
        type            patch;
        nFaces          10;
        startFace       7600;
    }
    upperWall
    {
        type            wall;
        inGroups        1(wall);
        nFaces          400;
        startFace       7610;
    }
    axis
    {
        type            empty;
        inGroups        1(empty);
        nFaces          0;
        startFace       8010;
    }
    front
    {
        type            wedge;
        inGroups        1(wedge);
        nFaces          4000;
        startFace       8010;
    }
    back
    {
        type            wedge;
        inGroups        1(wedge);
        nFaces          4000;
        startFace       12010;
    }
)

